{
  "$schema":"http://json-schema.org/draft-07/schema#",
  "$id":"https://bcrs.gov.bc.ca/.well_known/schemas/routing_slip_link_request",
  "type":"object",
  "title":"Routing slip search request",
  "required":[
    "childRoutingSlipNumber","parentRoutingSlipNumber"
  ],
  "properties":{
     "childRoutingSlipNumber":{
        "$id":"#/properties/childRoutingSlipNumber",
        "type":"string",
        "title":"child routing slip Number",
        "description":"child routing slip Number.",
        "default":"",
        "examples":[
           "REG00100100"
        ]
     },
      "parentRoutingSlipNumber":{
        "$id":"#/properties/parentRoutingSlipNumber",
        "type":"string",
        "title":"Parent routing slip Number",
        "description":"Parent routing slip Number.",
        "default":"",
        "examples":[
           "REG00100100"
        ]
     }
  }
}