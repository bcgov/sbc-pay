{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/account_info",
  "type": "object",
  "required": [
  ],
  "properties": {
    "accountId": {
      "$id": "#/properties/accountId",
      "type": "integer",
      "title": "BC Registries Account Number",
      "examples": [
        "180670"
      ],
      "pattern": "^(.*)$"
    },
    "accountName": {
      "$id": "#/properties/accountName",
      "type": "string",
      "title": "BC Registries Account Name",
      "examples": [
        "180670"
      ],
      "pattern": "^(.*)$"
    },
    "branchName": {
      "$id": "#/properties/branchName",
      "type": "string",
      "title": "BC Registries Branch Name",
      "examples": [
        "Big Branch"
      ],
      "pattern": "^(.*)$"
    },
    "bcolAccountNumber": {
      "$id": "#/properties/bcolAccountNumber",
      "type": "string",
      "title": "BC Online Account Number",
      "examples": [
        "180670"
      ],
      "pattern": "^(.*)$"
    },
    "bcolUserId": {
      "$id": "#/properties/bcolUserId",
      "type": "string",
      "title": "BC Online User ID",
      "examples": [
        "180670"
      ],
      "pattern": "^(.*)$"
    },
    "routingSlip": {
      "$id": "#/properties/routingSlip",
      "type": "string",
      "title": "Routing Slip Number",
      "examples": [
        "ABCD123456789"
      ],
      "pattern": "^(.*)$"
    },
    "datNumber": {
      "$id": "#/properties/datNumber",
      "type": "string",
      "title": "DAT Number for BCOL",
      "examples": [
        "C1234567890"
      ],
      "pattern": "^(.*)$"
    },
    "padTosAcceptedBy": {
      "$id": "#/properties/padTosAcceptedBy",
      "type": "string",
      "title": "PAD TOS Accepted users Id",
      "examples": [
        "1"
      ],
      "pattern": "^(.*)$"
    },
    "contactInfo": {
      "oneOf": [
        {
          "$ref": "https://bcrs.gov.bc.ca/.well_known/schemas/contact_info"
        }
      ]
    },
    "paymentInfo": {
      "oneOf": [
        {
          "$ref": "https://bcrs.gov.bc.ca/.well_known/schemas/payment_info"
        }
      ]
    }
  }
}
