{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/eft_short_name",
  "type": "object",
  "title": "EFT Short name patch request",
  "properties": {
    "casSupplierNumber": {
      "$id": "#/properties/casSupplierNumber",
      "type": "string",
      "title": "CAS Supplier Number",
      "description": "EFT Short name associated CAS supplier number.",
      "maxLength": 25,
      "examples": [
        "ABC123"
      ]
    },
    "casSupplierSite": {
      "$id": "#/properties/casSupplierSite",
      "type": "string",
      "title": "CAS Supplier Site",
      "description": "EFT Short name associated CAS supplier site.",
      "maxLength": 25,
      "examples": [
        "ABC123"
      ]
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "Email",
      "format": "email",
      "description": "EFT Short name contact email.",
      "maxLength": 100,
      "examples": [
        "test@gmail.com"
      ]
    }
  }
}
