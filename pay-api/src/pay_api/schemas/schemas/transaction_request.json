{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/transaction_request",
  "type": "object",
  "title": "Transaction Request",
  "required": [
    "clientSystemUrl",
    "payReturnUrl"
  ],
  "properties": {
    "clientSystemUrl": {
      "$id": "#/properties/transactions/items/properties/clientSystemUrl",
      "type": "string",
      "title": "Client System URL",
      "default": "",
      "examples": [
      ],
      "pattern": "^(.*)$"
    },
    "payReturnUrl": {
      "$id": "#/properties/transactions/items/properties/payReturnUrl",
      "type": "string",
      "title": "Payment Return URL",
      "default": "",
      "examples": [
      ],
      "pattern": "^(.*)$"
    }
  }
}