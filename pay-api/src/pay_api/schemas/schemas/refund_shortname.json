{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://bcrs.gov.bc.ca/.well_known/schemas/refund_shortname",
  "type": "object",
  "title": "Refund",
  "required":[
    "shortNameId", "refundAmount", "refundEmail", "refundMethod"
  ],
  "properties":{
    "short_name_id":{
      "$id":"#/properties/shortNameId",
      "type":"number",
      "title":"Short Name Id",
      "description":"short name id.",
      "default":"",
      "examples":[
        "24"
      ]
    },
    "refund_amount":{
      "$id":"#/properties/refundAmount",
      "type":"number",
      "title":"Refund Amount",
      "description":"refund amount.",
      "default":"",
      "examples":[
        10
      ]
    },
    "cas_supplier_number":{
      "$id":"#/properties/casSupplierNum",
      "type":"string",
      "title":"CAS Supplier Number",
      "description":"CAS supplier number.",
      "default":"",
      "examples":[
        "22901"
      ]
    },
    "cas_supplier_site":{
      "$id":"#/properties/casSupplierSite",
      "type":"string",
      "title":"CAS Supplier Site",
      "description":"CAS supplier site.",
      "default":"",
      "examples":[
        "22901"
      ]
    },
    "refund_email":{
      "$id":"#/properties/refundEmail",
      "type":"string",
      "title":"Refund Email",
      "description":"refund email.",
      "default":"",
      "examples":[
        "test@gmail.com"
      ]
    },
    "comment":{
      "$id":"#/properties/comment",
      "type":"string",
      "title":"Comment",
      "description":"comment.",
      "default":"",
      "examples":[
        "for comment test"
      ]
    },
    "refund_method":{
      "$id":"#/properties/refundMethod",
      "type":"string",
      "title":"Refund Method",
      "description":"refund method.",
      "default":"",
      "examples":[
        "CHEQUE",
        "EFT"
      ]
    },
    "entity_name":{
      "$id":"#/properties/entityName",
      "type":"string",
      "title":"Entity Name",
      "description":"entity name.",
      "default":"",
      "examples":[
        "test"
      ]
    },
    "street":{
      "$id":"#/properties/street",
      "type":"string",
      "title":"Street",
      "description":"street.",
      "default":"",
      "examples":[
        "1234"
      ]
    },
    "street_additional":{
      "$id":"#/properties/streetAdditional",
      "type":"string",
      "title":"Street Additional",
      "description":"street additional.",
      "default":"",
      "examples":[
        "1234"
      ]
    },
    "city":{
      "$id":"#/properties/city",
      "type":"string",
      "title":"City",
      "description":"city.",
      "default":"",
      "examples":[
        "vancouver"
      ]
    },
    "region":{
      "$id":"#/properties/region",
      "type":"string",
      "title":"Region",
      "description":"region.",
      "default":"",
      "examples":[
        "BC"
      ]
    },
    "postal_code":{
      "$id":"#/properties/postalCode",
      "type":"string",
      "title":"Postal Code",
      "description":"postal code.",
      "default":"",
      "examples":[
        "V5Z 1A1"
      ]
    },
    "country":{
      "$id":"#/properties/country",
      "type":"string",
      "title":"Country",
      "description":"country.",
      "default":"",
      "examples":[
        "Canada"
      ]
    },
    "delivery_instructions":{
      "$id":"#/properties/deliveryInstructions",
      "type":"string",
      "title":"Delivery Instructions",
      "description":"delivery instructions.",
      "default":"",
      "examples":[
        "for delivery instructions test"
      ]
    }
  }
}
